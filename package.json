{
  "name": "ppr",
  "version": "0.0.51",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ppr": "node dist/ppr/server/server.mjs",
    "fetch-data": "node fetch-json.js",
